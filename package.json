{
  "name": "test",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "mocha src/**/*.spec.ts --require ts-node/register --reporter spec",
    "start": "node index.js",
    "watch": "tsc -w",
    "serve": "nodemon -e ts --watch src --exec \"npm run build && npm run start\" "
  },
  "dependencies": {
    "@nestjs/common": "^3.0.3",
    "@nestjs/core": "^3.0.5",
    "@nestjs/microservices": "^3.0.2",
    "@nestjs/testing": "^3.0.3",
    "@nestjs/websockets": "^3.0.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.19",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sinon": "^2.4.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
